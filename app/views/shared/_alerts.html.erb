<% if flash[:alert] %>
  <%= js add_gritter(flash[:alert], title: "CameronReiter.com", sticky: false) %>
<% elsif flash[:error] %>
  <%= js add_gritter(flash[:error], title: "OOPS", sticky: false) %>
<% else %>  
  <%= js add_gritter(flash[:notice], title: "CameronReiter.com", sticky: false) %>
<% end %>